package com.company;

import java.util.Scanner;

public class Main {

    public static void main(String[] args) {

        String s = "";
        int sum = 0;
        Scanner scanner = new Scanner(System.in);
        int number = scanner.nextInt();
        System.out.println("___________________________1______________________________");
        int n = number;
        for (;n > 0; n--){
            int c = n;
            for (; c > 0; c--){
                int i = n;
                s = s + i + "";
            }
        }
        System.out.println(s);
        System.out.println("___________________________2______________________________");
        for (int i = 1; i < number; i++){
            System.out.println(i + "");
        }
        System.out.println();
        System.out.println("___________________________3______________________________");
        for (int i = 1; i <= number; i++){
            sum +=1;
        }
        System.out.println(sum);
    }
}
