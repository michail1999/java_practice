public class Cat {
    private String name;
    private int age;
    public Cat(String n, int a){
        name = n;
        age = a;
    }
    public Cat(String n){
        name = n;
        age = 0;
    }
    public Cat(){
        name = "Pup";
        age = 0;
    }
    public void setAge(int age) {
        this.age = age;
    }
    public void setName(String name) {
        this.name = name;
    }
    public String getName(String name){
        return name;
    }
    public int getAge() {
        return age;
    }
    public String toString(){
        return this.name+", age "+this.age;
    }
    public void intoHumanAge(){
        System.out.println(name+"'s age in human years is "+age*5+" years");
    }
}


public class TestCat {
    public static void main(String[] args) {
        Cat d1 = new Cat("Mike", 2);
        Cat d2 = new Cat("Helen", 7);
        Cat d3 = new Cat("Bob");
        d3.setAge(1);
        System.out.println(d1);
        d1.intoHumanAge();
        d2.intoHumanAge();
        d3.intoHumanAge();
    }
}
